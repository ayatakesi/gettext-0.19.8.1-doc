msgstr "--more-than=@var{number}"

#. type: opindex
#: original_texis/msgcomm.texi:69
#, no-wrap
msgid "->@r{, @code{msgcomm} option}"
msgstr "->@r{, @code{msgcomm} option}"

#. type: opindex
#: original_texis/msgcomm.texi:70
#, no-wrap
msgid "--more-than@r{, @code{msgcomm} option}"
msgstr "--more-than@r{, @code{msgcomm} option}"

#. type: table
#: original_texis/msgcomm.texi:73
msgid ""
"Print messages with more than @var{number} definitions, defaults to 1 if not "
"set."
msgstr ""
"@var{number}に指定した数より大きいメッセージを出力します。指定しなかった場合"
"のデフォルトは1です。"

#. type: item
#: original_texis/msgcomm.texi:74
#, no-wrap
msgid "-u"
msgstr "-u"

#. type: itemx
#: original_texis/msgcomm.texi:75
#, no-wrap
msgid "--unique"
msgstr "--unique"

#. type: opindex
#: original_texis/msgcomm.texi:76
#, no-wrap
msgid "-u@r{, @code{msgcomm} option}"
msgstr "-u@r{, @code{msgcomm} option}"

#. type: opindex
#: original_texis/msgcomm.texi:77
#, no-wrap
msgid "--unique@r{, @code{msgcomm} option}"
msgstr "--unique@r{, @code{msgcomm} option}"

#. type: table
#: original_texis/msgcomm.texi:80
