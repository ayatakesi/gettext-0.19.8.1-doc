#: original_texis/msgcat.texi:25
#, no-wrap
msgid "-f @var{file}"
msgstr "-f @var{file}"

#. type: itemx
#: original_texis/msgcat.texi:26
#, no-wrap
msgid "--files-from=@var{file}"
msgstr "--files-from=@var{file}"

#. type: opindex
#: original_texis/msgcat.texi:27
#, no-wrap
msgid "-f@r{, @code{msgcat} option}"
msgstr "-f@r{, @code{msgcat} option}"

#. type: opindex
#: original_texis/msgcat.texi:28
#, no-wrap
msgid "--files-from@r{, @code{msgcat} option}"
msgstr "--files-from@r{, @code{msgcat} option}"

#. type: table
#: original_texis/msgcat.texi:31
msgid ""
"Read the names of the input files from @var{file} instead of getting them "
"from the command line."
msgstr ""
"入力ファイルの名前を、コマンドラインからではなく、@var{file}から読み込みま"
"す。"

#. type: item
#: original_texis/msgcat.texi:32
#, no-wrap
msgid "-D @var{directory}"
msgstr "-D @var{directory}"

#. type: itemx
#: original_texis/msgcat.texi:33
#, no-wrap
msgid "--directory=@var{directory}"
msgstr "--directory=@var{directory}"

#. type: opindex
#: original_texis/msgcat.texi:34
#, no-wrap
