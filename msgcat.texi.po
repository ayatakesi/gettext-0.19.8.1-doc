#: original_texis/msgcat.texi:47
#, no-wrap
msgid "-o @var{file}"
msgstr "-o @var{file}"

#. type: itemx
#: original_texis/msgcat.texi:48
#, no-wrap
msgid "--output-file=@var{file}"
msgstr "--output-file=@var{file}"

