#. type: pindex
#: original_texis/msgunfmt.texi:1
#, no-wrap
msgid "msgunfmt"
msgstr "msgunfmt"

#. type: cindex
#: original_texis/msgunfmt.texi:2
#, no-wrap
msgid "@code{msgunfmt} program, usage"
msgstr "@code{msgunfmt} program, usage"

#. type: example
#: original_texis/msgunfmt.texi:5
#, no-wrap
msgid "msgunfmt [@var{option}] [@var{file}]...\n"
msgstr "msgunfmt [@var{option}] [@var{file}]...\n"

#. type: cindex
#: original_texis/msgunfmt.texi:7
#, no-wrap
msgid "convert binary message catalog into PO file"
msgstr "convert binary message catalog into PO file"

#. type: Plain text
#: original_texis/msgunfmt.texi:10
msgid ""
"The @code{msgunfmt} program converts a binary message catalog to a Uniforum "
"style .po file."
msgstr ""
"@code{msgunfmt}は、バイナリーのメッセージカタログを、Uniforum形式の.poファイ"
"ルに変換するプログラムです。"

