#: original_texis/msgunfmt.texi:18
#, no-wrap
msgid "Java mode, and @code{msgunfmt} program"
msgstr "Java mode, and @code{msgunfmt} program"

#. type: table
#: original_texis/msgunfmt.texi:20
msgid "Java mode: input is a Java @code{ResourceBundle} class."
msgstr "Javaモード: Javaの@code{ResourceBundle} class が入力となります。"

#. type: item
#: original_texis/msgunfmt.texi:21
#, no-wrap
msgid "--csharp"
msgstr "--csharp"

