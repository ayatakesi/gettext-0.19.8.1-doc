msgstr "--csharp-resources@r{, @code{msgunfmt} option}"

#. type: cindex
#: original_texis/msgunfmt.texi:29
#, no-wrap
msgid "C# resources mode, and @code{msgunfmt} program"
msgstr "C# resources mode, and @code{msgunfmt} program"

#. type: table
#: original_texis/msgunfmt.texi:31
msgid "C# resources mode: input is a .NET @file{.resources} file."
msgstr "C# resourcesモード: .NETの@file{.resources}ファイルが入力となります。"

#. type: item
#: original_texis/msgunfmt.texi:32
#, no-wrap
msgid "--tcl"
msgstr "--tcl"

#. type: opindex
#: original_texis/msgunfmt.texi:33
#, no-wrap
msgid "--tcl@r{, @code{msgunfmt} option}"
msgstr "--tcl@r{, @code{msgunfmt} option}"

#. type: cindex
#: original_texis/msgunfmt.texi:34
#, no-wrap
msgid "Tcl mode, and @code{msgunfmt} program"
msgstr "Tcl mode, and @code{msgunfmt} program"

#. type: table
#: original_texis/msgunfmt.texi:36
msgid "Tcl mode: input is a tcl/msgcat @file{.msg} file."
msgstr "Tclモード: tcl/msgcatの@file{.msg}ファイルが入力となります。"

#. type: subsection
#: original_texis/msgunfmt.texi:39
#, no-wrap
msgid "Input file location"
msgstr "Input file location"

#. type: item
#: original_texis/msgunfmt.texi:42
#, no-wrap
msgid "@var{file} @dots{}"
msgstr "@var{file} @dots{}"

#. type: table
#: original_texis/msgunfmt.texi:44
msgid "Input .mo files."
msgstr "入力となる.moファイルです。"

#. type: Plain text
#: original_texis/msgunfmt.texi:48
msgid ""
"If no input @var{file} is given or if it is @samp{-}, standard input is read."
msgstr ""
"@var{file}が指定されていないか、@samp{-}が指定された場合は、標準入力から読み"
"込みます。"

#. type: subsection
#: original_texis/msgunfmt.texi:49
#, no-wrap
msgid "Input file location in Java mode"
msgstr "Input file location in Java mode"

#. type: item
#: original_texis/msgunfmt.texi:52 original_texis/msgunfmt.texi:73
#, no-wrap
msgid "-r @var{resource}"
msgstr "-r @var{resource}"

#. type: itemx
#: original_texis/msgunfmt.texi:53 original_texis/msgunfmt.texi:74
#, no-wrap
msgid "--resource=@var{resource}"
msgstr "--resource=@var{resource}"

